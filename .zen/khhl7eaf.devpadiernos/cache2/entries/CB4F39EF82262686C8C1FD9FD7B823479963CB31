/* region Element Defaults */

body {
    background-color: var(--color-bg-page);
    font: var(--font-copy-md);
    color: var(--color-copy-primary);
}

p {
    margin: 0; /* We never want default margin */
}

h1, h2, h3, h4, h5, h6 {
    color: var(--color-heading);
    margin: 0; /* We never want default margin */
}

