						document.frmLogin.fldDate2.value = parent.date2;
						document.frmLogin.fldCheckSum.value = parent.CheckSum;
						document.frmLogin.fldDynamicUrl.value = parent.DynamicUrl;	/* ILENH-ADD-10/05/2011-SandeepS */
						document.frmLogin.fldDetails1.value = parent.Details1;
						document.frmLogin.fldDetails2.value = parent.Details2;
						document.frmLogin.fldDetails3.value = parent.Details3;
						document.frmLogin.fldDisplayDetails.value = parent.DisplayDetails;
						document.frmLogin.fldCustomerInvalidPass.value = parent.CustomerInvalidPass;  /* Flexenh 435- Invalid Customer Id error on Login Page Sanjana ADDED*/
					}

					try {
						document.frmLogin.fldEpiToken.value = '*' + parent.EpiReqId +
							'*' + parent.MerchantCode + '*' + parent.ClientCode + '*' + parent.MerchantRefNo + '*' + parent.TxnDateTime + '*' + parent.ref1 + '*' + parent.ref2 + '*' + parent.ref3 + '*' + parent.ref4 + '*' + parent.ref5 + '*';
						resetParentParams();
					} catch (e) { }

					if (parent.RsaAuthReq == 'Y') {
						document.frmLogin.fldTxnId.value = "RGN";
						document.frmLogin.fldScrnSeqNbr.value = "01";
						document.frmLogin.target = "_self";
					}
					if (!l_clicked) {
						document.frmLogin.submit();
					}
					break;
				case 'BROKER':
					document.frmLogin.fldApplicationId.value = parent.fldApplicationId;
					document.frmLogin.fldTransactionId.value = parent.fldTransactionId;
					document.frmLogin.fldAgencyId.value = parent.fldAgencyId;
					document.frmLogin.fldTxnRefNo.value = parent.fldTxnRefNo;
					document.frmLogin.fldSource.value = parent.fldSource;
					document.frmLogin.fldCodOrgBrn.value = parent.fldCodOrgBrn;
					document.frmLogin.fldHoldCustId.value = parent.fldHoldCustId;
					document.frmLogin.fldHoldAcctNo.value = parent.fldHoldAcctNo;
					document.frmLogin.fldHoldDesc.value = parent.fldHoldDesc;
					document.frmLogin.fldHoldAmount.value = parent.fldHoldAmount;
					document.frmLogin.fldHoldCurrency.value = parent.fldHoldCurrency;
					document.frmLogin.fldAppId.value = 'BR';
					if (!l_clicked) {
						document.frmLogin.target = "_parent";
						document.frmLogin.fldLoginUserId.disabled = false;
						document.frmLogin.submit();
						document.frmLogin.fldLoginUserId.disabled = true;
					}
					break;
				case 'DPHOLD':
					document.frmLogin.fldApplicationId.value = parent.fldApplicationId;
					document.frmLogin.fldTransactionId.value = parent.fldTransactionId;
					document.frmLogin.fldAgencyId.value = parent.fldAgencyId;
					document.frmLogin.fldTxnRefNo.value = parent.fldTxnRefNo;
					document.frmLogin.fldSource.value = parent.fldSource;
					document.frmLogin.fldMsgVer.value = parent.fldMsgVer;
					document.frmLogin.fldMsgTp.value = parent.fldMsgTp;
					document.frmLogin.fldRelId.value = parent.fldRelId;
					document.frmLogin.fldHoldCustId.value = parent.fldHoldCustId;
					document.frmLogin.fldDpTag.value = parent.fldDpTag;
					document.frmLogin.fldAppId.value = 'BR';
					document.frmLogin.fldScrnSeqNbr.value = '10';
					if (!l_clicked) {
						document.frmLogin.target = "_parent";
						document.frmLogin.fldLoginUserId.disabled = false;
						document.frmLogin.submit();
						document.frmLogin.fldLoginUserId.disabled = true;
					}

					break;

			
				/*FLEXENH - 499 | Ankit Gupta | START*/

				case 'NACH':

					document.frmLogin.fldAppId.value = 'XT';
					document.frmLogin.fldScrnSeqNbr.value = '61';
					document.frmLogin.target = "_parent";

					document.frmLogin.fldIsSplitPwdReq.value = parent.isSplitPwdReq;

					//console.log("user id " + document.frmLogin.fldLoginUserId.value);
					//console.log("split p " + parent.isSplitPwdReq);

					if (document.frmLogin.fldIsSplitPwdReq.value == 'Y') {

						//console.log("action will be nach");

						document.frmLogin.fldIsSplitPwdReq.value = "N";
						document.frmLogin.ClientAccNum.value = parent.ClientAccNum;
						document.frmLogin.MandateRefNo.value = parent.MandateRefNo;
						document.frmLogin.MerchantCode.value = parent.MerchantCode;
						document.frmLogin.Date.value = parent.Date;
						document.frmLogin.CheckSum.value = parent.CheckSum;
						document.frmLogin.TxnAmount.value = parent.TxnAmount;
						document.frmLogin.DebitType.value = parent.DebitType;
						document.frmLogin.DebitFreq.value = parent.DebitFreq;
						document.frmLogin.StartDate.value = parent.StartDate;
						document.frmLogin.EndDate.value = parent.EndDate;
						document.frmLogin.Category.value = parent.Category;
						document.frmLogin.AcctHolderName.value = parent.AcctHolderName;
						document.frmLogin.MerchantRefNo.value = parent.MerchantRefNo;
						document.frmLogin.MerchantName.value = parent.MerchantName;
						document.frmLogin.AcctType.value = parent.AcctType;
						document.frmLogin.Ref1.value = parent.Ref1;
						document.frmLogin.Ref2.value = parent.Ref2;
