						document.frmLogin.action = "merchant";

						var element = document.getElementsByName("fldDevicePrint")[0];	// To avoid query string max length check
						element.parentNode.removeChild(element);

					} else {

					
						document.frmLogin.OrgReqDate.value = parent.OrgReqDate;
						document.frmLogin.fldEpiReqId.value = parent.EpiReqId;
						document.frmLogin.fldRsaAuthReq.value = parent.RsaAuthReq;
						document.frmLogin.fldClientCode.value = parent.ClientCode;
