		//---------------------------------------------------------------------------------------
		function addField (form, fieldType, fieldName, fieldValue) {
		  var input = document.createElement('INPUT');
		   if (document.all) { 
		  input.type = fieldType;
		  input.name = fieldName;
		  input.value = fieldValue;
		   }
		   else if (document.getElementById) { 

		  input.setAttribute('type', fieldType);
		  input.setAttribute('name', fieldName);
		  input.setAttribute('value', fieldValue);
		    }
		  form.appendChild(input);
		}
		//FLEXENH-674-06/11/2020-RS-CTA handling for BB Redirection-END //06-04-2024 adi FLEXPROD-114:ISG ISSUE
		//---------------------------------------------------------------------------------------
		function fSubmit(e) {

			var key;
			if (window.event) {
				key = window.event.keyCode; //IE
			} else {
				key = e.which;			//firefox
			}

			if (key != 13) {
				return true;
			}

			if (parent.daemon == 'NETBANKING' || parent.RsaAuthReq == 'Y') {
				return fLogon();
			}
			return true;
		}
		//---------------------------------------------------------------------------------------
		function fLogon() {
			//FLEXENH-674-06/11/2020-RS-CTA handling for BB Redirection-START  //06-04-2024 adi FLEXPROD-114:ISG ISSUE
			if(parent.daemon == "NETBANKING" && parent.redirectParam!= undefined){
				var redirectParamVal = ltrim(rtrim(parent.redirectParam));
				if(redirectParamVal!=''){
					addField (document.frmLogin, "hidden", "fldBBRedirectCTAParam", redirectParamVal);
				}
			}
			//FLEXENH-674-06/11/2020-RS-CTA handling for BB Redirection-END //06-04-2024 adi FLEXPROD-114:ISG ISSUE
			// Anant Mane : RSA Device Print :start
			var l_dp_value;
			try {
				l_dp_value = encode_deviceprint();
			} catch (e) {
				l_dp_value = '';
			}
			document.frmLogin.fldDevicePrint.value = l_dp_value;
			// Anant Mane : RSA Device Print :end
			//console.log('[fLogon] document.frmLogin.fldLoginUserId.value.trim() : ' + document.frmLogin.fldLoginUserId.value.trim());
			//document.frmLogin.fldLoginUserId.value = document.frmLogin.fldLoginUserId.value.trim();

			document.frmLogin.fldLoginUserId.value = ltrim(rtrim(document.frmLogin.fldLoginUserId.value));


			var l_str_val = isStringValid(document.frmLogin.fldLoginUserId.value, "required, minlen=1, maxlen=15,alphanumat", true, "Customer ID ");//  06-june-2016 Pratik Kashid FS_CCR_0498_Added menumonic for Personalization of user id  --maxlen=10,salphanum
			if (l_str_val != null) {
				document.frmLogin.fldLoginUserId.focus();
				return false;
			}

	/*ADD : TEJAS D - ISG Vulnerability Issue START*/
			if( /[^a-zA-Z0-9@._]/.test( document.frmLogin.fldLoginUserId.value ) ) {
                  alert('Customer ID  should contain Alphabets or Numbers or three special characters (. or _ or @)');
                  document.frmLogin.fldLoginUserId.focus();	   
                  return false;
				  } 
	/*ADD : TEJAS D - ISG Vulnerability Issue END*/

			//if redirection to BB is required, skip password field validation/check

			if ((parent.daemon == "MERCHANT" && ((parent.isSplitPwdReq != 'R') && (parent.isSplitPwdReq != 'Y')) && (parent.RsaAuthReq == 'N' || parent.RsaAuthReq == 'E'))  //FLXENH-410 Pratik
				|| ((parent.daemon == "NSDL") && ((parent.isSplitPwdReq != 'R') && (parent.isSplitPwdReq != 'Y'))) 
                                || (parent.daemon == "BROKER")
				|| (parent.daemon == "DPHOLD") || (parent.daemon == "CBEC") || (parent.daemon == "ICEGATE")
				|| (((parent.daemon == "GSTN") || (parent.daemon == "GST1")) && ((parent.isSplitPwdReq != 'R') && (parent.isSplitPwdReq != 'Y')))
				|| (parent.daemon == "TIN" && ((parent.isSplitPwdReq != 'R') && (parent.isSplitPwdReq != 'Y')) && (parent.RsaAuthReq == 'N' || parent.RsaAuthReq == 'D' || parent.RsaAuthReq == 'E'))		//FLEXENH-656 | Ankit | TIN Integration
				|| ((parent.daemon == "NACH") && ((parent.isSplitPwdReq != 'R') && (parent.isSplitPwdReq != 'Y')))) { /* Hitesh FLEXENH-274  GSTN*/	// Ankit | Split password page for NACH

				var l_str_val = isStringValid(document.frmLogin.fldPassword.value, "required, minlen=4", true, "IPIN (password)");
				if (l_str_val != null) {
					document.frmLogin.fldPassword.focus();
					return false;
				}
			}
			/*ILENH-PasswordPolicy-22/11/2012-SandeepSaxena-Start*/

			//if redirection to BB is required, skip password field encr

			if ((parent.daemon == "MERCHANT" && ((parent.isSplitPwdReq != 'R') && (parent.isSplitPwdReq != 'Y')) && (parent.RsaAuthReq == 'N' || parent.RsaAuthReq == 'D' || parent.RsaAuthReq == 'E')) //FLXENH-410 Pratik
				|| ((parent.daemon == "NSDL") && ((parent.isSplitPwdReq != 'R') && (parent.isSplitPwdReq != 'Y'))) || (parent.daemon == "BROKER")
				|| (parent.daemon == "DPHOLD") || (parent.daemon == "CBEC") || (parent.daemon == "ICEGATE")
				|| (((parent.daemon == "GSTN") || (parent.daemon == "GST1")) && ((parent.isSplitPwdReq != 'R') && (parent.isSplitPwdReq != 'Y')))
				|| (parent.daemon == "TIN" && ((parent.isSplitPwdReq != 'R') && (parent.isSplitPwdReq != 'Y')) && (parent.RsaAuthReq == 'N' || parent.RsaAuthReq == 'D' || parent.RsaAuthReq == 'E'))		//FLEXENH-656 | Ankit | TIN Integration
				|| ((parent.daemon == "NACH") && ((parent.isSplitPwdReq != 'R') && (parent.isSplitPwdReq != 'Y')))) {	  /* Hitesh FLEXENH-274  GSTN*/	// Ankit | Split password page for NACH

				var randomnumber = Math.floor(10000000 + (Math.random() * (100000000 - 10000000 + 1)));
				var l_str_pas = stringToHex(des(document.frmLogin.fldPassword.value.substring(0, 8), document.frmLogin.fldPassword.value, 1, 0));
				var l_sub_password = l_str_pas.toUpperCase().substring(2, l_str_pas.length);
				while (l_sub_password.length < 32) {
					l_sub_password = l_sub_password + "0";
				}
				document.frmLogin.fldRandomNumber.value = randomnumber;
				document.frmLogin.fldPassword.value = hex_hmac(document.frmLogin.fldRandomNumber.value, l_sub_password);
			}
			/*ILENH-PasswordPolicy-22/11/2012-SandeepSaxena-END*/
