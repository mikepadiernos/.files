		//-------------------------------------------------------------------------------
	function showVirtualKeyBoard(){
		var checkbox = document.getElementById("chksecmod");
		if(checkbox.checked == true){
			document.getElementById("keyboardDiv").style.display= "block";
			document.frmLogin.fldPassword.readOnly =true;
		    document.frmLogin.fldPassword.blur();
		}
		else{
			document.getElementById("keyboardDiv").style.display= "none";
			document.frmLogin.fldPassword.readOnly = false;
			if (false == confirm("Use of Security Keyboard is recommended to protect your Password. Are you sure you choose NOT to use the Security Keyboard?")) {
					document.frmLogin.chksecmod.checked = true;
					showVirtualKeyBoard();
				}
		}
	}

		//----------------------------------------------------------------------------------
		function init() {


			document.getElementById("nortonimg2").style.display="none"; 	//FLEXENH-426-18/02/2020-Roshni Soni-EPI Split Page

				if (parent.daemon != 'NETBANKING' && (parent.RsaAuthReq == '' || parent.RsaAuthReq == 'N' || parent.RsaAuthReq == 'D' || parent.RsaAuthReq == 'E')) {
					setKeyboardFocus("frmLogin", "fldPassword");
				}
				//FLEXENH-426-18/02/2020-Roshni Soni-EPI Split Page-START
				if (parent.daemon != 'NETBANKING'){
				
					document.getElementById("hdfcwelcmetxtId").innerHTML="Pay through HDFC Bank NetBanking";

					document.getElementById("welcomesec1").style.display="none";

					document.getElementById("welcomesec2").style.display="none";

					document.getElementById("nortonimg1").style.display="none";

					document.getElementById("nortonimg2").style.display="block";

				}
				//FLEXENH-426-18/02/2020-Roshni Soni-EPI Split Page-END

				// Ankit | Split password page for GSTN | Start

				if ((parent.daemon == 'GSTN') || (parent.daemon == 'GST1')) {

					if (parent.idUser != null && parent.idUser.length > 0) {

						document.frmLogin.fldLoginUserId.value = parent.idUser;
						document.frmLogin.fldLoginUserId.readOnly = true;
					}

					// Approach 2
					/*
					if (parent.isSplitPwdReq == 'R') {
					return fLogon();
					}
					*/

					if (parent.RsaAuthReq == 'Y') {
						document.frmLogin.fldIsSplitPwdReq.value = 'N';
					}
				}
				// Ankit | Split password page for GSTN | END
				
				// Ankit | Split password page for TIN | START

				if (parent.daemon == 'TIN') {

				if (parent.idUser != null && parent.idUser.length > 0) {

					document.frmLogin.fldLoginUserId.value = parent.idUser;
					document.frmLogin.fldLoginUserId.readOnly = true;
				}

				// Approach 2
				/*
				if (parent.isSplitPwdReq == 'R') {
				return fLogon();
				}
				*/

				if (parent.RsaAuthReq == 'Y') {
					document.frmLogin.fldIsSplitPwdReq.value = 'N';
				}
				}
				// Ankit | Split password page for TIN | End

				if (parent.daemon == 'NETBANKING' || parent.daemon == 'MERCHANT' || parent.daemon == 'NSDL' || parent.daemon == 'NACH') {
					document.frmLogin.fldLoginUserId.value = "";

					// Ankit | Split password page for non rsa epi | Start

					if (parent.idUser != null && parent.idUser.length > 0) {

						document.frmLogin.fldLoginUserId.value = parent.idUser;
						document.frmLogin.fldLoginUserId.readOnly = true;
					}

					// Approach 2
					/*
					if (parent.isSplitPwdReq == 'R') {
						return fLogon();
					}
					*/

					if (parent.RsaAuthReq == 'Y') {
						document.frmLogin.fldIsSplitPwdReq.value = 'N';
					}

					// Ankit | Split password page for non rsa epi | End

					document.frmLogin.fldLoginUserId.focus();
					if (parent.daemon == 'MERCHANT' && (parent.RsaAuthReq == 'N' || parent.RsaAuthReq == 'E')) {
						if (document.frmLogin.chksecmod.checked) {
							document.frmLogin.fldPassword.readOnly = true;
							document.frmLogin.fldPassword.blur();
							document.frmLogin.fldPassword.focus();
						} else {
							document.frmLogin.fldPassword.readOnly = false;
						}
					}
				} else if (parent.daemon == 'BROKER' || parent.daemon == 'DPHOLD') {
					document.frmLogin.fldLoginUserId.value = parent.fldHoldCustId;
					document.frmLogin.fldLoginUserId.disabled = true;
					if (parent.daemon == 'BROKER') {						
						if (document.frmLogin.chksecmod.checked) {
							document.frmLogin.fldPassword.readOnly = true;
							document.frmLogin.fldPassword.blur();
							document.frmLogin.fldPassword.focus();
						} else {
							document.frmLogin.fldPassword.readOnly = false;
							document.frmLogin.fldPassword.focus();
						}
					} else {
						document.frmLogin.fldPassword.focus();
					}
				}			 
			return false;
		}
		//-----------------------------------------------------------------------------
		function DeletePwd() {
			if (document.frmLogin.chksecmod.checked) {
				var strNumField = new String(document.frmLogin.fldPassword.value);
				document.frmLogin.fldPassword.value = strNumField.substring(0, strNumField.length - 1);
				document.frmLogin.fldPassword.id = "";
				doRandomize();
			}
			return false;
		}

		//-----------------------------------------------------------------------------
		function ClearPwd() {
			if (document.frmLogin.chksecmod.checked) {
				document.frmLogin.fldPassword.value = "";
				doRandomize();
			}
			return false;
		}
		//-----------------------------------------------------------------------------
		function SecurityWarning() {
			if (document.frmLogin.chksecmod.checked) {
				document.frmLogin.fldPassword.readOnly = true;
				document.frmLogin.fldPassword.blur();
			} else {
				document.frmLogin.fldPassword.readOnly = false;
				if (false == confirm("Use of Security Keyboard is recommended to protect your Password. Are you sure you choose NOT to use the Security Keyboard?")) {
					document.frmLogin.chksecmod.checked = true;
					SecurityWarning();
				}
			}
		}
		//-----------------------------------------------------------------------------
		function changeIndicator(q) {
			var l_caps_indicator;
			l_caps_indicator = document.getElementById("capsindicator");

			if (q == 0) {
				l_caps_indicator.innerHTML = "ON";
			} else {
				l_caps_indicator.innerHTML = "OFF";
			}
		}
		//-------------------------------------------------------------------------------
		function resetParentParams() {
			try {
				var tmp_str = '';
				parent.OrgReqDate = tmp_str;
				parent.EpiReqId = tmp_str;
				parent.ref1 = tmp_str;
				parent.ref2 = tmp_str;
				parent.ref3 = tmp_str;
				parent.ref4 = tmp_str;
				parent.ref5 = tmp_str;
				parent.ref6 = tmp_str;
				parent.ref7 = tmp_str;
				parent.ref8 = tmp_str;
				parent.ref9 = tmp_str;
				parent.ref10 = tmp_str;
				parent.ref11 = tmp_str;
			} catch (e) { }
		}
		/*02/03/2015	RogerG	ENH CCR 431: Retrieve Customer Id  Start */
		function fretrieveCustId() {
			document.frmLogin.fldAppId.value = "RS";
			document.frmLogin.fldTxnId.value = "CRO";
			document.frmLogin.fldScrnSeqNbr.value = "01";
			document.frmLogin.submit();
			return false;

		}
/*02/03/2015	RogerG	ENH CCR 431: Retrieve Customer Id  end */
		/* mehul start */
		function fretrieveCustIdEpi() {
			var w = window.open('', '_target', '');
			document.frmLogin.target = "_target";

			document.frmLogin.fldAppId.value = "RS";
			document.frmLogin.fldTxnId.value = "CRO";
			document.frmLogin.fldScrnSeqNbr.value = "01";
			document.frmLogin.submit();
			return false;

		}
